<Window x:Class="GraphPathFinding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphPathFinding"
        mc:Ignorable="d"
        Title="MainWindow" Height="1050" Width="1400">
	<StackPanel>
		<Button x:Name="PRMButton" Content="PRM Algorithm" Height="30" Width="200" Click="Button_Click_PRM"/>
		<Button x:Name="Button" Content="Dijkstra Algorithm" Height="30" Width="200" Click="Button_Click_Dijkstra" RenderTransformOrigin="1.026,0.433" Margin="596,0"/>
		<Button x:Name="RasterButton" Content="Raster Algorithm" Height="30" Width="200" Click="RasterButton_Click"/>
		<Button x:Name="Composition" Content="Composition Algorithm" Height="30" Width="200" Click="Composition_Click"/>
		<Image x:Name="WorldMap" Height="800"/>


	</StackPanel>
</Window>
